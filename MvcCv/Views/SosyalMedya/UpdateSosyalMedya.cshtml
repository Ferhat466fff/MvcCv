@model MvcCv.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "UpdateEgitim";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Sosyal Medya Güncelleme Sayfası</h4>
<br />
<form method="post" class="form-group">

    @Html.HiddenFor(x => x.ID)

    <label>Sosyal Medya İsmi</label>
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    <label>Link</label>
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control"})
    <br />
    <label>İkon</label>
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control"})
    <br />
  
    <button class="btn btn-outline-success"><i class="fas fa-save"></i>Güncelle</button>
</form>


